<h1>Listing talk_records</h1>

<table>
  <tr>
    <th>Student</th>
    <th>About</th>
    <th>Memo</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @talk_records.each do |talk_record| %>
  <tr>
    <td><%= talk_record.student_id %></td>
    <td><%= talk_record.about %></td>
    <td><%= talk_record.memo %></td>
    <td><%= link_to 'Show', talk_record %></td>
    <td><%= link_to 'Edit', edit_talk_record_path(talk_record) %></td>
    <td><%= link_to 'Destroy', talk_record, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

